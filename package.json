{
  "name": "tetris-ui",
  "version": "0.1.5",
  "private": false,
  "description": "tetris-ui is an react components lib base on antd",
  "author": "linmingdao",
  "license": "MIT",
  "keywords": [
    "Component",
    "UI",
    "React",
    "Antd",
    "tetris-ui"
  ],
  "homepage": "https://github.com/linmingdao/tetris-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/linmingdao/tetris-ui"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "node scripts/start.js",
    "clean": "rimraf ./dist",
    "lint": "eslint --ext js,ts,tsx src --max-warnings 5",
    "lint-staged": "lint-staged",
    "build": "npm run clean && npm run build-ts && npm run build-css",
    "test": "node scripts/test.js",
    "test:nowatch": "cross-env CI=true node scripts/test.js",
    "build-ts": "tsc -p tsconfig.build.json",
    "build-css": "node-sass ./src/styles/index.scss ./dist/index.css",
    "format": "prettier --write 'src/**/*{.js,.jsx,.ts,.tsx,.css,.less,.scss,.sass}'",
    "prettier": "npm run format",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "commitmsg": "commitlint -x @commitlint/config-conventional -E GIT_PARAMS",
    "prepublishOnly": "npm run test:nowatch && npm run lint-staged && npm run build",
    "release": "npm run build && npm publish && git push && git push origin --tags ",
    "release-beta": "npm run build && npm publish --tag=beta && git push && git push origin --tags ",
    "release-patch": "npm version patch -m 'docs: 发布修订版本%s' && npm run build && && npm publish && git push && git push origin --tags ",
    "release-minor": "npm version minor -m 'docs: 发布次版本%s' && npm run build && && npm publish && git push && git push origin --tags ",
    "release-major": "npm version major -m 'docs: 发布主版本%s' && npm run build && && npm publish && git push && git push origin --tags ",
    "prerelease-beta": "npm version prerelease -m 'docs: 预发布版本%s' && npm run build && npm publish --tag=beta && git push && git push origin --tags"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "lodash.clonedeep": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "nanoid": "^3.1.23",
    "pubsub-js": "^1.9.3",
    "react-dnd": "^14.0.2",
    "react-dnd-html5-backend": "^14.0.0",
    "react-flow-renderer": "^9.5.3",
    "react-loadable": "^5.5.0",
    "validator": "^13.6.0"
  },
  "peerDependencies": {
    "antd": ">=4.8.5",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "react-router-dom": ">=5.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@pmmmwh/react-refresh-webpack-plugin": "0.4.3",
    "@storybook/addon-actions": "6.2.8",
    "@storybook/addon-essentials": "6.2.8",
    "@storybook/addon-links": "6.2.8",
    "@storybook/react": "6.2.8",
    "@svgr/webpack": "5.5.0",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.22",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.debounce": "^4.0.6",
    "@types/node": "^12.20.10",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-loadable": "^5.5.5",
    "@types/react-router-dom": "^5.1.7",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "antd": "^4.15.2",
    "antd-dayjs-webpack-plugin": "^1.0.6",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.0",
    "babel-loader": "8.1.0",
    "babel-plugin-named-asset-import": "^0.3.7",
    "babel-preset-react-app": "^10.0.0",
    "bfj": "^7.0.2",
    "camelcase": "^6.1.0",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "cross-env": "^7.0.3",
    "css-loader": "4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "eslint": "^7.11.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.9.2",
    "eslint-webpack-plugin": "^2.5.2",
    "file-loader": "6.1.1",
    "fs-extra": "^9.0.1",
    "html-webpack-plugin": "4.5.0",
    "husky": "^6.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.0",
    "jest-circus": "26.6.0",
    "jest-resolve": "26.6.0",
    "jest-watch-typeahead": "0.6.1",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "0.11.3",
    "node-sass": "^5.0.0",
    "optimize-css-assets-webpack-plugin": "5.0.4",
    "pnp-webpack-plugin": "1.6.4",
    "postcss-flexbugs-fixes": "4.2.1",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "5.0.2",
    "prompts": "2.4.0",
    "react": "^17.0.2",
    "react-app-polyfill": "^2.0.0",
    "react-dev-utils": "^11.0.3",
    "react-dom": "^17.0.2",
    "react-refresh": "^0.8.3",
    "react-router-dom": "^5.2.0",
    "resolve": "1.18.1",
    "resolve-url-loader": "^3.1.2",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.0.5",
    "semver": "7.3.2",
    "style-loader": "1.3.0",
    "terser-webpack-plugin": "4.2.3",
    "ts-pnp": "1.2.0",
    "typescript": "4.2.4",
    "url-loader": "4.1.1",
    "web-vitals": "^1.1.1",
    "webpack": "4.44.2",
    "webpack-dev-server": "3.11.1",
    "webpack-manifest-plugin": "2.2.0",
    "workbox-webpack-plugin": "5.1.4"
  },
  "lint-staged": {
    "*.{js,jsx,md,json}": [
      "prettier --write",
      "git add"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testRunner": "<rootDir>/node_modules/jest-circus/runner.js",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
